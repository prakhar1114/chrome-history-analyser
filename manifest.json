{
    "manifest_version": 3,
    "name": "My Chrome Usage Analyser",
    "version": "1.0",
    "description": "Creates an amazing dashboard for your history over time.",
    "permissions": [
      "history",
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "src/background/background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "src/popup/popup.html"
    }
  }